{
  "name": "webpack-starter-angular",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "clean": "npm cache clean && rimraf -- doc coverage build",

    "start": "webpack-dev-server --hot --inline --port 3000 --content-base build/ --config webpack.dev",
    "start:build": "",
    "build": "npm run clean && webpack --config webpack.prod",
    "build:prod": "npm run clean &&  webpack -p --config webpack.prod",

    "lint": "eslint src/ || true",
    "lint:travis": "eslint src/",

    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "keywords": [
    "bundle",
    "webpack",
    "npm",
    "angular",
    "es2015"
  ],
  "author": "Marcin Kopacz",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "~1.5.0",

    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-import": "*",

    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.3.0",

    "node-sass": "*",
    "raw-loader": "*",
    "css-loader": "*",
    "sass-loader": "*",
    "script-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "file-loader": "*",

    "webpack": "^2.1.0-beta",
    "webpack-merge": "*",
    "webpack-dev-server": "^2.0.0-beta",
    "html-webpack-plugin": "*",
    "ng-annotate-webpack-plugin": "*",
    "extract-text-webpack-plugin": "^2.0.0-beta"
  },
  "dependencies": {
    "angular": "1.x.x",
    "angular-sanitize": "1.x.x",
    "angular-animate": "1.x.x",
    "angular-aria": "1.x.x",
    "angular-material": "1.x.x",
    "angular-moment": "^1.0.0-beta",

    "angular-ui-router": "^1.0.0-beta",
    "angular-ui-grid": "3.x.x",

    "lodash": "4.x.x"
  }
}
