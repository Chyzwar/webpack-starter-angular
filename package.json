{
  "name": "main",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "webpack": "./node_modules/.bin/webpack",
    "webpack:server": "./node_modules/.bin/webpack-dev-server",

    "analyze": "npm run webpack -- --json | webpack-bundle-size-analyzer",
    "clean": "npm cache clean && rm -rf -- doc coverage build",

    "start": "npm run webpack:server -- --hot --inline --port 3000 --content-base build/ --config ./webpack/dev",
    "start:build": "npm run webpack:server -- --hot --inline --port 3000 --content-base build/ --config ./webpack/prod",
    "build": "npm run clean && npm run webpack -- --config ./webpack/prod",
    "build:prod": "npm run clean &&  npm run webpack -- -p --config ./webpack/prod",

    "lint": "npm run eslint -- src/",
    "lint:travis": "npm run eslint -- src/",

    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "keywords": [
    "bundle",
    "webpack",
    "angular"
  ],
  "author": "Marcin Kopacz",
  "license": "MIT",
  "repository": "https://github.com/Chyzwar/webpack-starter-angular",
  "devDependencies": {
    "eslint": "3.13.x",
    "eslint-config-airbnb": "14.0.x",
    "eslint-plugin-react": "6.9.x",
    "eslint-plugin-jsx-a11y": "3.x.x",
    "eslint-plugin-import": "2.2.x",

    "karma": "1.x.x",
    "karma-chai": "*",
    "karma-mocha": "1.x.x",
    "karma-mocha-reporter": "1.x.x",
    "karma-chrome-launcher": "1.x.x",
    "karma-firefox-launcher": "1.x.x",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "1.x.x",
    "mocha": "3.x.x",
    "chai": "3.x.x",
    "angular-mocks": "1.x.x",

    "node-sass": "*",
    "raw-loader": "*",
    "css-loader": "*",
    "sass-loader": "*",
    "script-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "file-loader": "*",

    "webpack": "2.x.x",
    "webpack-merge": "*",
    "webpack-dev-server": "2.x.x",
    "html-webpack-plugin": "*",
    "ng-annotate-webpack-plugin": "*",
    "extract-text-webpack-plugin": "^2.0.0-rc"
  },
  "dependencies": {
    "angular": "1.x.x",
    "angular-sanitize": "1.x.x",
    "angular-animate": "1.x.x",
    "angular-aria": "1.x.x",
    "angular-material": "1.x.x",
    "angular-moment": "^1.x.x",

    "angular-ui-router": "^1.0.0-rc",
    "lodash": "4.x.x"
  }
}
