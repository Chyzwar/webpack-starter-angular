{
  "name": "webpack-starter-angular",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "clean": "npm cache clean && rimraf -- node_modules doc coverage build",

    "start": "webpack-dev-server --hot --inline --port 3000 --content-base build/ --config webpack.dev" ,
    "build": "webpack --config webpack.prod",
    "build:production": "webpack -p --config webpack.prod",

    "lint": "eslint src/ || true",
    "lint:travis": "eslint src/",

    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "keywords": [
    "bundle",
    "webpack",
    "npm",
    "angular"
  ],
  "author": "Marcin Kopacz",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "~1.5.0",

    "eslint": "*",
    "eslint-plugin-react": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-import": "*",

    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.3.0",

    "node-sass": "^3.3.3",
    "raw-loader": "^0.5.1",
    "css-loader": "^0.21.0",
    "sass-loader": "^3.0.0",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "file-loader": "^0.8.4",

    "webpack": "2.1.0-beta.15",
    "webpack-merge": "^0.14.0",
    "webpack-dev-server": "*",
    "html-webpack-plugin": "^1.7.0",
  },
  "dependencies": {
    "angular": "1.5.7",
    "angular-sanitize": "1.5.7",
    "angular-schema-form": "0.8.13",
    "angular-schema-form-bootstrap": "0.2.0",
    "angular-material": "*",
    "angular-ui-bootstrap": "1.1.2",
    "angular-ui-router": "0.2.18",

    "bootstrap": "3.3.6",
    "jquery": "2.2.4",
    "lodash": "*",
    "objectpath": "1.2.1",
    "tv4": "*"
  }
}
